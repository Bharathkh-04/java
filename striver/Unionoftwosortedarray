class Solution {
    public int[] unionArray(int[] nums1, int[] nums2) {
      Set<Integer> union=new HashSet<>();
      for(int i=0;i<nums1.length;i++)
      {
        union.add(nums1[i]);
      }  
      for(int i=0;i<nums2.length;i++)
      {
        union.add(nums2[i]);
      }
      int [] result=new int[union.size()];
      int i=0;
      for(int num:union)
      {
        result[i++]=num;
      }
      return result;
    }
}
