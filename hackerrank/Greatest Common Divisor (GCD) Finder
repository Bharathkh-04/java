import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int small=sc.nextInt();
        int great=sc.nextInt();
        int temp=0;
        while(great!=0)
        {
            if(great<small)
            {
                temp=great;
                great=small;
                small=temp;
            }
            great=great%small;
        }System.out.println(small);
    }
}
