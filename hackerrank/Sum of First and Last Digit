import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int n=Math.abs(sc.nextInt());
        int sum=n%10;
        while(n>=10)
        {
           n=n/10;
        }
        System.out.println(sum+n);
    }
}
