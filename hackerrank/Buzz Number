import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
    Scanner sc=new Scanner(System.in);
    int n=sc.nextInt();
    int a=n%10;
    if(n%7==0 || a==7)
    {
        System.out.println("Buzz Number");
    }
    else
    {
        System.out.println("Not Buzz Number");
    }
}
}
