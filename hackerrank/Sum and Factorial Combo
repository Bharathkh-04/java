import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
       Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int count=0;
        int fact=1;
        for(int i=1;i<=n;i++)
        {
            count+=i;
            fact=fact*i;
        }
        System.out.println(count);
        System.out.println(fact);
    }
}
